;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	dns.acuminous.meh. root.localhost. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	dns.acuminous.meh.
201	IN	PTR	laserjet.acuminous.meh
220	IN	PTR	hn1.acuminous.meh


